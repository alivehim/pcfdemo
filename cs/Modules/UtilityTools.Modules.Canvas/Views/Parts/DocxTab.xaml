<ui:RibbonTab x:Class="UtilityTools.Modules.Canvas.Views.Parts.DocxTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UtilityTools.Modules.Canvas.Views"
             xmlns:ui="clr-namespace:UtilityTools.Controls.Ribbon.UI;assembly=UtilityTools.Controls.Ribbon"
             mc:Ignorable="d" 
             Header="Docx"   
             ContextualBrush="Chocolate"
             d:DesignHeight="450" d:DesignWidth="800">

    <ui:RibbonPage>
        <ui:RibbonGroup Header="Func">
            <ui:RibbonButton Command="{Binding OpenDocxCommand}"
                          Content="OpenDocx"
                          ImageSource="/UtilityTools.Modules.Canvas;component/Resources/Images/flow.png" >
            </ui:RibbonButton>

           
        </ui:RibbonGroup>
    </ui:RibbonPage>

</ui:RibbonTab>
